{
  "name": "sams-flags",
  "version": "0.0.0",
  "private": true,
  "description": "Monorepo for the sams-flags learning project",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "oxlint .",
    "lint:fix": "oxlint --fix .",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "fmt": "oxfmt .",
    "fmt:check": "oxfmt --check .",
    "format": "pnpm run fmt:check"
  },
  "devDependencies": {
    "@oxfmt/binding-darwin-arm64": "^0.31.0",
    "@oxlint/binding-darwin-arm64": "^1.46.0",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "oxfmt": "^0.31.0",
    "oxlint": "^1.46.0",
    "tsx": "^4.19.2",
    "turbo": "^2.4.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@8.15.9"
}
